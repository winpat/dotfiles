---

all:
 - .emacs
 - .vimrc
 - .tmux.conf
 - .bash_profile
 - .bashrc
 - .inputrc
 - .config/nixpkgs
 - .config/git

gem|tron|clu:
 - .Xmodmap
 - .gtkrc-2.0
 - .ideavimrc
 - .config/dunst
 - .config/zathura
 - .config/alacritty
 - .ipython/profile_default/startup/start.py
 - nixos/configuration.nix -> /etc/nixos/configuration.nix
 - nixos/cachix.nix -> /etc/nixos/cachix.nix
 - nixos/cachix -> /etc/nixos/cachix

tron:
 - .config/i3
 - .config/i3blocks
 - .config/openbox
 - .config/tint2
 - nixos/tron-configuration.nix -> /etc/nixos/host-configuration.nix

clu:
 - .config/openbox
 - .config/tint2
 - nixos/clu-configuration.nix -> /etc/nixos/host-configuration.nix

mcp:
 - .mbsyncrc
 - .mail.el

gem:
 - .config/i3
 - .config/i3blocks
 - .config/openbox
 - .config/tint2
 - nixos/gem-configuration.nix -> /etc/nixos/host-configuration.nix
