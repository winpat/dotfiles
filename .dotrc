---

all:
 - .emacs
 - .vimrc
 - .tmux.conf
 - .bash_profile
 - .bashrc
 - .inputrc
 - .config/nixpkgs
 - .config/git

tron|clu:
 - .Xmodmap
 - .gtkrc-2.0
 - .ideavimrc
 - .mbsyncrc
 - .mail.el
 - .config/dunst
 - .config/zathura
 - .config/termite
 - nixos/configuration.nix -> /etc/nixos/configuration.nix

tron:
 - .config/i3
 - .config/i3blocks
 - .config/openbox
 - .config/tint2
 - nixos/tron-configuration.nix -> /etc/nixos/host-configuration.nix

clu:
 - .config/openbox
 - .config/tint2
 - nixos/clu-configuration.nix -> /etc/nixos/host-configuration.nix